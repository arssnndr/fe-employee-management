{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/fe-employee-management/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/employee-management$",
      "status": 301,
      "headers": { "Location": "/employee-management/" }
    },
    {
      "src": "/employee-management/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))$",
      "dest": "/$1"
    },
    {
      "src": "/employee-management/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "/employee-management/(.*)$",
      "dest": "/employee-management/index.html"
    },
    {
      "src": "/$",
      "status": 301,
      "headers": { "Location": "/employee-management/" }
    },
    {
      "src": "/(.*)$",
      "dest": "/employee-management/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|mjs))$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*\\.css)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
