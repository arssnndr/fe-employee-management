{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/fe-employee-management/browser"
      }
    }
  ],
  "redirects": [
    {
      "source": "/employee-management",
      "destination": "/employee-management/",
      "permanent": true
    },
    {
      "source": "/",
      "destination": "/employee-management/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/employee-management/(.*).js",
      "destination": "/$1.js"
    },
    {
      "source": "/employee-management/(.*).css",
      "destination": "/$1.css"
    },
    {
      "source": "/employee-management/(.*).ico",
      "destination": "/$1.ico"
    },
    {
      "source": "/employee-management/(.*).png",
      "destination": "/$1.png"
    },
    {
      "source": "/employee-management/(.*).jpg",
      "destination": "/$1.jpg"
    },
    {
      "source": "/employee-management/(.*).jpeg",
      "destination": "/$1.jpeg"
    },
    {
      "source": "/employee-management/(.*).gif",
      "destination": "/$1.gif"
    },
    {
      "source": "/employee-management/(.*).svg",
      "destination": "/$1.svg"
    },
    {
      "source": "/employee-management/(.*).woff",
      "destination": "/$1.woff"
    },
    {
      "source": "/employee-management/(.*).woff2",
      "destination": "/$1.woff2"
    },
    {
      "source": "/employee-management/(.*).ttf",
      "destination": "/$1.ttf"
    },
    {
      "source": "/employee-management/(.*).eot",
      "destination": "/$1.eot"
    },
    {
      "source": "/employee-management/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/employee-management/(.*)",
      "destination": "/employee-management/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/employee-management/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*).mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
